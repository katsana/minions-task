{
    "name": "katsana/minions-task",
    "description": "Minions Task Manager for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@katsana.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Minions\\Task\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minions\\Task\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0 || ^7.0",
        "katsana/minions": "^1.7 || ^2.3"
    },
    "require-dev": {
        "orchestra/canvas": "^4.5 || ^5.0",
        "orchestra/testbench": "^4.5 || ^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Minions\\Task\\TaskServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
